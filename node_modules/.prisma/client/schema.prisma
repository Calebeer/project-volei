datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model farmacia{
  id    Int    @id @default(autoincrement())
  nome String

  preco String
  quantidade String
}

model user{
  id Int @id @default(autoincrement())
  usuario String @unique
  senha String 
  
}

model cliente{
  id Int @id @default(autoincrement())
  nome String 
  cpf String @unique
  endereco String
  telefone String
}

model venda{
  id Int @id @default(autoincrement())
  cpf String @unique
  nome_remedio String
  quantidade String
}

