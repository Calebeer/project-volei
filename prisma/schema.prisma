datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int       @id @default(autoincrement())
  name       String
  address    String
  telephone  String
  email      String    @unique
  password   String
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt
}

model Scheduling {
  id      Int        @id @default(autoincrement())
  name    String
  userId  Int
  date    DateTime
  time    DateTime
}




